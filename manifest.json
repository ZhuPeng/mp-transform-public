{
   "name": "小程序排版小助手",
   "version": "2.9",
   "description": "微信公众号文章一键轻松排版小程序链接。",
   "manifest_version": 2,
   "homepage_url": "https://github.com/ZhuPeng/mp-transform-public",
   "icons": { 
       "16": "img/link-16.png",
       "32": "img/link-32.png",
       "64": "img/link-64.png",
       "128": "img/link-128.png"
   },  
   "permissions": ["activeTab", "clipboardWrite"],
   "browser_action": {},
   "background": {
       "scripts": ["background.js"],
       "persistent": false
   },
   "content_scripts": [{
       "matches": ["https://itra.run/RunnerSpace/RaceResults/*"],
       "js": ["itra.js"]
  }]
}
